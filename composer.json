{
    "name": "yabsta/omnipay-encoded",
    "description": "Gateway For encoded payment system",
    "keywords": [
        "omnipay",
        "encoded"
    ],
    "homepage": "https://github.com/alecksey/omnipay-encoded",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksii Bogatchenko",
            "email": "delphiworld@gmail.com",
            "homepage": "https://www.yabsta.com"
        }
    ],
    "require": {
        "php": "^5.6|^7|^8",
        "omnipay/common": "~3.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "league/iso3166": "^4.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Encoded\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Encoded\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
